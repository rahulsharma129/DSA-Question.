class Solution {
public:
    vector<int> maxKDistinct(vector<int>& nums, int k) {
        sort(nums.begin(),nums.end());
        set<int>ans;
        int n=nums.size();
        for(int i=n-1;i>=0 && k>0;i--){
            if(ans.find(nums[i])==ans.end()){
            ans.insert(nums[i]);
            k--;
            }
        }
        vector<int>res(ans.rbegin(),ans.rend());
         return res;
    }
};